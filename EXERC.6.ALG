Algoritmo "exerc.6"

Var
    vel,m,vm: real



Inicio
       escreval("Digite a velocidade do veículo:")
       leia(vel)
       
       vm <- 434.4/ vel
       m <- vel/ 3.6
       
       escreval("O percurso de São Paulo até Rio de Janeiro durará: ",vm, " horas")
       escreval("Velocidade em m/s:", m)



Fimalgoritmo