Algoritmo "nome"

Var
    nome,sobrenome,soma: caracter


Inicio
      escreval("Escreva seu primeiro nome: ")
      leia(nome)
      
      escreval("Escreva seu sobrenome: ")
      leia(sobrenome)
      
      soma<- nome + sobrenome
      
      escreval("Seu nome completo é: ",soma)

       //escreva o nome e dê um espaço

Fimalgoritmo